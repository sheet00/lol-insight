{
  "name": "lol-teacher",
  "version": "1.0.0",
  "description": "League of Legends Teacher Application",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "generate": "nuxt generate",
    "preview": "npm run build && wrangler dev --config ./wrangler.jsonc",
    "postinstall": "nuxt prepare",
    "deploy": "npm run build && wrangler deploy --config ./wrangler.jsonc",
    "cf-typegen": "wrangler types",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply lol-teacher-db --local --config ./wrangler.jsonc",
    "db:migrate:prod": "wrangler d1 migrations apply lol-teacher-db --config ./wrangler.jsonc"
  },
  "devDependencies": {
    "@cloudflare/d1": "^1.4.1",
    "@nuxt/devtools": "^2.6.3",
    "@types/better-sqlite3": "^7.6.13",
    "@volar/typescript": "^2.4.23",
    "better-sqlite3": "^12.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.4",
    "nuxt": "^4.0.3",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "@types/uuid": "^10.0.0",
    "nuxt-auth-utils": "^0.5.23",
    "openai": "^5.15.0",
    "pinia": "^3.0.3",
    "uuid": "^11.1.0"
  },
  "overrides": {
    "replace-in-file": "^8.3.0",
    "glob": "^10.4.5"
  }
}
