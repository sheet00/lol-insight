{
  "name": "lol-teacher",
  "version": "1.0.0",
  "description": "League of Legends Teacher Application",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host 0.0.0.0 --port 3000",
    "generate": "nuxt generate",
    "preview": "npm run build && wrangler dev --config ./wrangler.jsonc",
    "postinstall": "nuxt prepare",
    "deploy": "npm run build && wrangler deploy --config ./wrangler.jsonc",
    "cf-typegen": "wrangler types",
    "_postinstall_disabled": "nuxt prepare && prisma generate",
    "_db:migrate": "prisma migrate dev",
    "_db:generate": "prisma generate",
    "_db:push": "prisma db push",
    "_db:studio": "prisma studio"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@volar/typescript": "^2.4.23",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.4",
    "nuxt": "^4.0.3",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.5",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@pinia/nuxt": "^0.11.2",
    "nuxt-auth-utils": "^0.5.23",
    "openai": "^5.12.2",
    "pinia": "^3.0.3"
  }
}
